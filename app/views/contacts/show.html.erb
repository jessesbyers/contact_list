<h1><%= @contact.name %></h1>

<h3>Mailing Address: <%= @contact.address %></h3>
<br>
<h3>Phone Number: <%= @contact.phone %></h3>
<br>
<h3>Email Address: <%= @contact.email %></h3>
<br>

<h3>Pets:</h3>
<ul>
    <% @contact.pets.each do |pet| %>
        <li><%= pet.name %> : <%= pet.breed %></li>
        <br>
    <% end %>
</ul>